V-OBSH-LA ; 

V-BUDU-LA ; 

V-PROS-NA ; 
V-PROS-NA-NEG ; 
V-PROS-NA-GNA ; 

%<imp%>:%>й # ;
%<imp%>%<neg%>:%>ге # ;

LEXICON V-INE-UNA-INA-ANA-E

