LEXICON V-OBSH-LA
! Общее время 
%<aor%>:%>ла # ; 
%<aor%>%<neg%>:%>ларо # ; 
%<gpr1%>:%>ле V-GPR-DECL ; 
%<gpr1%>%<neg%>:%>ларе V-GPR-DECL ; 
%<gna1%>:%>лаго # ; 
%<gna1%>%<neg%>:%>ларого # ; 

LEXICON V-BUDU-LA 
! Будущее время
%<fut%>:%>ла # ; 
%<fut%>%<neg%>:%>ларо # ; 
%<gpr3%>:%>ле V-GPR-DECL ; 
%<gpr3%>%<neg%>:%>ларе V-GPR-DECL ; 

LEXICON V-OBSH-NA
! Общее время 
%<aor%>:%>на # ; 
%<aor%>%<neg%>:%>наро # ; 
%<gpr1%>:%>не V-GPR-DECL ; 
%<gpr1%>%<neg%>:%>раре V-GPR-DECL ; 
%<gna1%>:%>наго # ; 
%<gna1%>%<neg%>:%>нарого # ; 

LEXICON V-BUDU-NA 
! Будущее время
%<fut%>:%>на # ; 
%<fut%>%<neg%>:%>наро # ; 
%<gpr3%>:%>не V-GPR-DECL ; 
%<gpr3%>%<neg%>:%>наре V-GPR-DECL ; 

LEXICON V-PROS-NA
! Прощедшее время 
%<past%>:%>на # ; 
%<gpr2%>:%>ра V-GPR-DECL ; 

LEXICON V-PROS-NA-NEG
! Отрицательная форма прощедшего времени
%<past%>%<neg%>:%>чӀо # ; 
%<gpr2%>%<neg%>:%>чӀе V-GPR-DECL ; 

LEXICON V-PROS-NA-GNA
! Деепричастие прощедшего времени
%<gna2%>:%>н # ; 

